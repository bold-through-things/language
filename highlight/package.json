{
    "name": "67lang_syntax_highlight",
    "displayName": "67LANG Syntax Highlighting",
    "description": "can you see it now?",
    "version": "0.0.1",
    "engines": {
        "vscode": ">=0.9.0-pre.1"
    },
    "publisher": "u1tmt",
    "contributes": {
        "languages": [{
            "id": "67lang",
            "aliases": ["67LANG"],
            "extensions": [".ind"]
        }],
        "grammars": [{
            "language": "67lang",
            "scopeName": "source.67lang",
            "path": "./syntaxes/main.67lang.tmLanguage.json"
        }],
        "configurationDefaults": {
			"[67lang]": {
				"editor.insertSpaces": false,
				"editor.tabSize": 2,
				"editor.detectIndentation": false
			}
		}
    }
}