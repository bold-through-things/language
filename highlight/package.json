{
    "name": "indentifire_syntax_highlight",
    "displayName": "INDENTIFIRE Syntax Highlighting",
    "description": "can you see it now?",
    "version": "0.0.1",
    "engines": {
        "vscode": ">=0.9.0-pre.1"
    },
    "publisher": "u1tmt",
    "contributes": {
        "languages": [{
            "id": "indentifire",
            "aliases": ["INDENTIFIRE"],
            "extensions": [".ind"]
        }],
        "grammars": [{
            "language": "indentifire",
            "scopeName": "source.indentifire",
            "path": "./syntaxes/main.indentifire.tmLanguage.json"
        }],
        "configurationDefaults": {
			"[indentifire]": {
				"editor.insertSpaces": false,
				"editor.tabSize": 2,
				"editor.detectIndentation": false
			}
		}
    }
}