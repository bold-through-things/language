{
    "name": "67lang_syntax_highlight",
    "displayName": "67lang Syntax Highlighting",
    "description": "can you see it now?",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.80.0"
    },
    "main": "./extension.js",
    "activationEvents": [
        "onLanguage:67lang"
    ],
    "publisher": "status.call.h8r",
    "contributes": {
        "languages": [{
            "id": "67lang",
            "extensions": [".67lang"]
        }],
        "grammars": [{
            "language": "67lang",
            "scopeName": "source.67lang",
            "path": "./syntaxes/main.67lang.tmLanguage.json"
        }],
        "configurationDefaults": {
			"[67lang]": {
				"editor.insertSpaces": false,
				"editor.tabSize": 4,
				"editor.detectIndentation": false,
                "editor.trimAutoWhitespace": false
			}
		},
        "commands": [
            {
                "command": "67lang.formatDocument",
                "title": "Format 67lang Document"
            }
        ]
    }
}