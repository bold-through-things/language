note test the new dict and set inline syntax

local user
	dict
		type named str for K
		type named str for V
		entry
			string "name"
			string "Bob"
		entry
			string "age"
			string "30"
		entry
			string "city"
			string "New York"

pls do print
	pls get user

local my_set
	pls do set
		string "apple"
		string "banana"
		string "cherry"

pls do print
	pls get my_set

local my_list
	list
		type named int
		note this one is implicit append
		int 0
		append
			int 1
		prepend
			int -2
		insert_after_index 0
			int -1
		note again append implicitly
		int 2
		int 3

pls do print
	pls get my_list

note test simple list (no operations)
local simple_list
	list
		type named int
		int 1
		int 2
		int 3

pls do print
	pls get simple_list
