fn process_list
	param my_list
		type named list
			type named str
	returns
		type named list
			type named str
	# Test that we can call slice on a typed list parameter
	# This should resolve the ambiguous overload issue
	local copied_list
		pls do slice the my_list
	
	# Test that we can call reverse on the list
	pls do reverse the copied_list
	
	return
		pls get copied_list

# Test the function with a list
local test_list
	list
		type named str
		string "a"
		string "b"
		string "c"

local result
	pls do process_list
		pls get test_list

pls do print
	pls get result
