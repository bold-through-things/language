note test the new dict and set inline syntax

local user
	dict
		type str for K
		type str for V
		entry
			string "name"
			string "Bob"
		entry
			string "age"
			string "30"
		entry
			string "city"
			string "New York"

do print
	get user

local my_set
	do new_set
		string "apple"
		string "banana"
		string "cherry"

do print
	get my_set

local my_list
	list
		type int
		note this one is implicit append
		int 0
		append
			int 1
		prepend
			int -2
		insert_after_index 0
			int -1
		note again append implicitly
		int 2
		int 3

do print
	get my_list

