	do URL
		string "https://example.com/path?query=123"

do print
	get pathname from myURL

do print
	string "\n--- Testing TextEncoder/TextDecoder ---"

local encoder
	do TextEncoder

local decoder
	do TextDecoder

local text_tests
	list
		type str
		string "ASCII text"
		string "Unicode: ðŸ¦• Deno"
		string "Numbers: 42"

for text in
	get text_tests
	do
		
		
		local bytes
			do encode the encoder
				get text

		local restored
			do decode the decoder
				get bytes

		do print
			string `Text: "`
			get text
			string `" -> Bytes: [`
			get bytes
			string `] -> Restored: "`
			get restored

do print
	string "\n--- Testing atob/btoa ---"

local testString
	string "Hello World"

local encodedString
	do btoa
		get testString

local decodedString
	do atob
		get encodedString

do print
	string "Original: "
	get testString
	string " -> Encoded: "
	get encodedString
	string " -> Decoded: "
	get decodedString

do print
	string "\n--- Testing JSON.stringify/parse ---"

local testObj
	dict
		type str for K
		type str for V
		entry
			string "name"
			string "Claude"
		entry
			string "type"
			string "AI"
		entry
			string "version"
			string "42"

local jsonString
	do JSON.stringify
		get testObj

local parsedObj
	67lang:assume_type_valid
	type dict
		type str
		type *
	do JSON.parse
		get jsonString

do print
	string "Original object -> JSON: "
	get jsonString

do print
	string "Parsed back -> name: "
	do # the parsedObj
		string "name"

