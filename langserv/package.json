{
    "name": "67lang_lsp",
    "displayName": "67LANG Language Server",
    "description": "a better language.",
    "publisher": "u1tmt",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.80.0"
    },
    "activationEvents": [
        "onLanguage:67lang"
    ],
    "main": "./client/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "67lang",
                "aliases": [
                    "67LANG"
                ],
                "extensions": [
                    ".ind"
                ]
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "vscode-languageclient": "^8.1.0"
    }
}
